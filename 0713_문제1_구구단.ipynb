    "import random\n",
    "\n",
    "x = random.randrange(1,101)\n",
    "\n",
    "while True:\n",
    "    \n",
    "    print('숫자를 맞춰보세요(1-100)')\n",
    "    y = int(input())\n",
    "    if y > x:\n",
    "        print('숫자가 너무 큽니다.')\n",
    "    elif y < x:\n",
    "        print('숫자가 너무 작습니다')\n",
    "    else:\n",
    "        print('정답입니다. 입력한 숫자는 {0}입니다'.format(y))\n",
    "        break"
   ]
  }
